package Day11;

    class A
    {
    	// int a=100;
    	 int a;
    	 void display()
    	 {
    		 System.out.println(a);
    	 }
    }
    	 class B extends A
    	 {
    		 int b=200;
    		 void show()
    		 {
    			System.out.println(b); 
    		 }
    	 }
    	 
    	
    
public class InhertianceType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		B bobj= new B();
		// System.out.println(bobj.a);
		System.out.println(bobj.b);
		
		bobj.a=500;
		
		bobj.display();
		bobj.show();
	
		

	}

}
